<package>
	<name>kraken_msgs</name>
	<version>1.1.0</version>
	<description>kraken_msgs</description>
	<maintainer email="prudhvi@auviitkgp.local">Prudhvi</maintainer>

	<license>BSD</license>

	<url type="website">http://ros.org/wiki/kraken_msgs</url>
